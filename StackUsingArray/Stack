public class Stack{
    private int arr[]=new int[1000];
    private int top=-1;
    public void push(int a){
        arr[top+1]=a;
        top++;
    }
    public void pop(){
        top--;
    }
    public void peek(){
        System.out.println(arr[top]);
    }
    public void display(){
        for(int i=0;i<=top;i++){
            System.out.println(arr[i]);
        }
    }
}
