
public class Node{
    private int data;
    private Node prev;
    private Node next;
    public Node(int a){
        data=a;
    }
    public int getData(){
        return data;
    }
    public Node getPrev(){
        return prev;
    }
    public Node getNext(){
        return next;
    }
    public void setPrev(Node a){
        prev=a;
    }
    public void setNext(Node a){
        next=a;
    }
}


